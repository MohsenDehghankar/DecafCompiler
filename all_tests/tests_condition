int main(){

    int x;
    x = 4;
    int y;
    y = 7;
    print(x>y);
    print(x>4);
    print(x>=4);
    print((x-1) > y);
    print(x!=y);
    print(x==y);

}



    
---------------
false
false
true
false
true
